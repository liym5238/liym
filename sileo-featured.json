<script>
    // 计算网站从今天开始运行的天数
    var launchDate = new Date(); // 开始时间设置为当前时间
    launchDate.setDate(launchDate.getDate() - 1); // 减去一天，从今天开始算
    var currentDate = new Date();
    var differenceInTime = currentDate - launchDate;
    var differenceInDays = Math.ceil(differenceInTime / (1000 * 3600 * 24));
    document.getElementById('daysRunning').textContent = differenceInDays;
</script>

